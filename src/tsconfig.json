{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2018",
    "baseUrl": ".",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": true,
    "importHelpers": true,
    "emitDecoratorMetadata": true,
    "downlevelIteration": true,
    "experimentalDecorators": true
  }
}